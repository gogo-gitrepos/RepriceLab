# Testing Environment Variables
# Copy this file to .env.test and fill in your values

# Frontend URL
BASE_URL=http://localhost:5000

# Backend API URL  
API_URL=http://localhost:8000

# Test User Credentials (create a test user in your dev environment)
TEST_USER_EMAIL=test@repricelab.com
TEST_USER_PASSWORD=TestPassword123!

# Test Admin Credentials (optional)
TEST_ADMIN_EMAIL=admin@repricelab.com
TEST_ADMIN_PASSWORD=AdminPassword123!

# Database (for API tests)
TEST_DATABASE_URL=postgresql://user:password@localhost:5432/repricelab_test

# JWT Secret (should match backend)
JWT_SECRET_KEY=test_secret_key_for_testing_only

# Timeouts (in milliseconds)
DEFAULT_TIMEOUT=30000
NAVIGATION_TIMEOUT=10000
